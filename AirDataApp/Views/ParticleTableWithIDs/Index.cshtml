@model IEnumerable<AirDataApp.Models.ParticleTableWithID>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-hover" >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DatoMaerke)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StofNavn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resultat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DatoMaerke)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StofNavn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resultat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
